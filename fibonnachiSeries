let nTerm=6;
let first=0 , second=1;

if(nTerm==1){
    console.log(first);
}
if(nTerm==2){
    console.log(first);
    console.log(second);
}else{
    console.log(first);
    console.log(second);
    for(let i=0;i<(nTerm-2);i++){
        let term=first+second;
        first=second;
        second=term;
        console.log(term);
   }
}

